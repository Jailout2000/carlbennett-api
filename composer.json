{
    "name": "carlbennett/carlbennett-api",
    "description": "API code that powers api.carlbennett.me",
    "type": "project",
    "keywords": [
        "carl",
        "bennett",
        "api",
        "automation",
        "integration"
    ],
    "homepage": "https://github.com/carlbennett/carlbennett-api",
    "license": "AGPLv3",
    "authors": [
        {
            "name": "Carl Bennett",
            "email": "carl@carlbennett.me"
        }
    ],
    "config": {
        "vendor-dir": "lib"
    },
    "autoload": {
        "psr-4": {
            "CarlBennett\\API\\": "src/"
        }
    },
    "require": {
        "erusev/parsedown": "^1.7",
        "ext-curl": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-pdo": "*",
        "geoip2/geoip2": "~2.0",
        "php-64bit": "^8.1",
        "phpmailer/phpmailer": "^6.0",
        "ramsey/uuid": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
